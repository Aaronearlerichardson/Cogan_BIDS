{"dataFormat" : [".nii", ".nii.gz", ".gz", ".edf", ".mgz", ".txt", ".mat",".dat.gz",".dat"]
, "compress" : true
, "compressLevel" : 6
, "repetitionTimeInSec" : 2
, "delayTimeInSec" : [ 0.028, 0.013, 0.026, 0.040]
, "series" : "odd-interleaved"
, "anat": {"left" : ".*?"
    , "right" : ".*?"
    , "content" : [ ["T1w","T1"]
                    ,["CT","CT"]
]
    }   
, "ieeg": {"left" : ".*?"
            , "right" : ".*?"
            , "content" : [   ["ieeg",".dat.gz"]
                               , ["ieeg",".dat"]
                            ]
            , "binary?" : true
            , "binaryEncoding": "float32"
            , "headerFile": "experiment\\.mat"
            , "headerData": "channels.name"
            , "sampleRate": "recording.sample_rate"
            }
, "ieeg.task": {"left" : "_"
            , "right" : ".*?"
            , "content" : [ ["Phoneme_Sequence","PHONEMESEQUENCE"]
                            ,["Phoneme_Sequence", "PhonemeSequence"]
                            ,["Phoneme_Sequence", "PhonemeSequencing"]
]
    }
, "ieeg.loc":{"left" : ".*?"
    , "right" : ".*?"
    , "content" : [   ["RAS",".txt"]
                    ]
    }
            
, "sessLabel" : {"left" : "^[0-9]*?"
                , "right" : "\\.[(edf)(mat)]"
                , "content" :   ["[0-9]{46}"]
                }
, "runIndex" : {"left" : "run"
                , "right" : "_"
                , "content" : ["[0-9]{1,3}"]
                , "fill" : 2
                , "repeat" : false
                }
, "partLabel" : {"left" : ".*?"
                , "right" : "[_ ]"
                , "content" : ["D[0-9]{1,4}"]
                , "fill": 4
                }
, "echo" : {"left" : "e"
    , "right" : "\\."
    , "content" : ["[0-9]{2}"]
    }
, "acq" : {"left" : "Session"
    , "right" : "[_ \\.]"
    , "content" : ["[0-9]{1,3}"]
    , "fill": 2
    }
, "ce" : {"left" : "_%&#$%"
    , "right" : "_$#^#^"
    , "content" : ["[0-9]{6}"]
    }    
,"eventFiles" : "([Tt]rial(s|Info).*?\\.mat)"
,"eventFormat" : ["Subject", "Trial", "Rec", "Day", "FilenamePrefix", "Start",
    "Auditory", "Go", "StartCode", "AuditoryCode", "GoCode", "Noisy", "NoResponse",
    "ResponseStart","ResponseEnd","cue", "sound", "go", "cueTime", "block", "cueStart",
     "Trigger", "cueEnd", "audioStart", "audioAlignedTrigger", "delStart", "delEnd",
      "goStart", "goEnd", "respStart", "respEnd", "isiStart", "isiEnd", "tCaptureStart"]
,"eventFormat.IDcol" : "FilenamePrefix"
,"eventFormat.Sep" : { "runIndex" : "block",
                             "acq" : "Rec" }
}
